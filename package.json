{
  "name": "slidev-theme-modern-tech",
  "version": "0.0.1",
  "repository": {"url": "https://github.com/kacper-olszanski/slidev-modern-tech"},
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.40.14",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.4",
    "theme-vitesse": "^0.6.4"
  },
  "devDependencies": {
    "@slidev/cli": "^0.40.14"
  },
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "serif": "Roboto Slab",
        "sans": "Bernina Sans, Roboto",
        "local": "Bernina Sans",
        "mono": "Roboto Mono"
      },
      "hightlighter": "prism"
    }
  }
}